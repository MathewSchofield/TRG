@article{bovy_galactic_2016,
	title = {On {Galactic} density modeling in the presence of dust extinction},
	volume = {818},
	issn = {1538-4357},
	url = {http://arxiv.org/abs/1509.06751},
	doi = {10.3847/0004-637X/818/2/130},
	abstract = {Inferences about the spatial density or phase-space structure of stellar populations in the Milky Way require a precise determination of the effective survey volume. The volume observed by surveys such as Gaia or near-infrared spectroscopic surveys, which have good coverage of the Galactic mid-plane region, is highly complex because of the abundant small-scale structure in the three-dimensional interstellar dust extinction. We introduce a novel framework for analyzing the importance of small-scale structure in the extinction. This formalism demonstrates that the spatially-complex effect of extinction on the selection function of a pencil-beam or contiguous sky survey is equivalent to a low-pass filtering of the extinction-affected selection function with the smooth density field. We find that the angular resolution of current 3D extinction maps is sufficient for analyzing Gaia sub-samples of millions of stars. However, the current distance resolution is inadequate and needs to be improved by an order of magnitude, especially in the inner Galaxy. We also present a practical and efficient method for properly taking the effect of extinction into account in analyses of Galactic structure through an effective selection function. We illustrate its use with the selection function of red-clump stars in APOGEE using and comparing a variety of current 3D extinction maps.},
	number = {2},
	urldate = {2016-11-01},
	journal = {The Astrophysical Journal},
	author = {Bovy, Jo and Rix, Hans-Walter and Green, Gregory M. and Schlafly, Edward F. and Finkbeiner, Douglas P.},
	month = feb,
	year = {2016},
	note = {arXiv: 1509.06751},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	pages = {130},
	annote = {Comment: Code available at https://github.com/jobovy/mwdust and at https://github.com/jobovy/apogee-maps},
	file = {arXiv\:1509.06751 PDF:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/CF7VPDJ8/Bovy et al. - 2016 - On Galactic density modeling in the presence of du.pdf:application/pdf;arXiv.org Snapshot:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/PGZWVKD6/1509.html:text/html}
}

