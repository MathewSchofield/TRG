@article{flower_transformations_1996,
	title = {Transformations from {Theoretical} {Hertzsprung}-{Russell} {Diagrams} to {Color}-{Magnitude} {Diagrams}: {Effective} {Temperatures}, {B}-{V} {Colors}, and {Bolometric} {Corrections}},
