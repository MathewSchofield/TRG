}

@article{yu_asteroseismology_2016,
	title = {Asteroseismology of 1523 misclassified red giants using \${\textbackslash}textit\{{Kepler}\}\$ data},
	issn = {0035-8711, 1365-2966},
	url = {http://arxiv.org/abs/1608.05803},
	doi = {10.1093/mnras/stw2074},
	abstract = {We analysed solar-like oscillations in 1523 \${\textbackslash}textit\{Kepler\}\$ red giants which have previously been misclassified as subgiants, with predicted \${\textbackslash}nu\_\{{\textbackslash}rm max\}\$ values (based on the Kepler Input Catalogue) between 280\${\textbackslash}mu\$Hz to 700\${\textbackslash}mu\$Hz. We report the discovery of 626 new oscillating red giants in our sample, in addition to 897 oscillators that were previously characterized by Hekker et al. (2011) from one quarter of \${\textbackslash}textit\{Kepler\}\$ data. Our sample increases the known number of oscillating low-luminosity red giants by \$26{\textbackslash}\%\$ (up to \${\textbackslash}sim\$ 1900 stars). About three quarters of our sample are classified as ascending red-giant-branch stars, while the remainder are red-clump stars. A novel scheme was applied to determine \${\textbackslash}Delta {\textbackslash}nu\$ for 108 stars with \${\textbackslash}nu\_\{{\textbackslash}rm max\}\$ close to the Nyquist frequency (240\${\textbackslash}mu\$Hz {\textless} \${\textbackslash}nu\_\{{\textbackslash}rm max\}\$ {\textless} 320\${\textbackslash}mu\$Hz). Additionally, we identified 47 stars oscillating in the super-Nyquist frequency regime, up to 387\${\textbackslash}mu\$Hz, using long-cadence light curves. We show that the misclassifications are most likely due to large uncertainties in KIC surface gravities, and do not result from the absence of broadband colors or from different physical properties such as reddening, spatial distribution, mass or metallicity. The sample will be valuable to study oscillations in low-luminosity red giants and to characterize planet candidates around those stars.},
	urldate = {2016-08-24},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Yu, Jie and Huber, Daniel and Bedding, Timothy R. and Stello, Dennis and Murphy, Simon J. and Xiang, Maosheng and Bi, Shaolan and Li, Tanda},
	month = aug,
	year = {2016},
	note = {arXiv: 1608.05803},
	keywords = {Astrophysics - Solar and Stellar Astrophysics},
	pages = {stw2074},
	annote = {Comment: Accepted for publication in MNRAS, 12 pages, 12 figures, 1 table},
	file = {arXiv\:1608.05803 PDF:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/KS4J4UTG/Yu et al. - 2016 - Asteroseismology of 1523 misclassified red giants .pdf:application/pdf;arXiv.org Snapshot:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/FM3TEABD/1608.html:text/html}
}

