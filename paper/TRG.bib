}

@article{verner_global_2011,
	title = {Global asteroseismic properties of solar-like oscillations observed by {Kepler} : {A} comparison of complementary analysis methods},
	volume = {415},
	issn = {00358711},
	shorttitle = {Global asteroseismic properties of solar-like oscillations observed by {Kepler}},
	url = {http://arxiv.org/abs/1105.0571},
	doi = {10.1111/j.1365-2966.2011.18968.x},
	abstract = {We present the asteroseismic analysis of 1948 F-, G- and K-type main-sequence and subgiant stars observed by the NASA \{{\textbackslash}em Kepler Mission\}. We detect and characterise solar-like oscillations in 642 of these stars. This represents the largest cohort of main-sequence and subgiant solar-like oscillators observed to date. The photometric observations are analysed using the methods developed by nine independent research teams. The results are combined to validate the determined global asteroseismic parameters and calculate the relative precision by which the parameters can be obtained. We correlate the relative number of detected solar-like oscillators with stellar parameters from the \{{\textbackslash}em Kepler Input Catalog\} and find a deficiency for stars with effective temperatures in the range \$5300 {\textbackslash}lesssim T\_{\textbackslash}mathrm\{eff\} {\textbackslash}lesssim 5700\${\textbackslash},K and a drop-off in detected oscillations in stars approaching the red edge of the classical instability strip. We compare the power-law relationships between the frequency of peak power, \${\textbackslash}nu\_{\textbackslash}mathrm\{max\}\$, the mean large frequency separation, \${\textbackslash}Delta{\textbackslash}nu\$, and the maximum mode amplitude, \$A\_{\textbackslash}mathrm\{max\}\$, and show that there are significant method-dependent differences in the results obtained. This illustrates the need for multiple complementary analysis methods to be used to assess the robustness and reproducibility of results derived from global asteroseismic parameters.},
	number = {4},
	urldate = {2017-02-02},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Verner, G. A. and Elsworth, Y. and Chaplin, W. J. and Campante, T. L. and Corsaro, E. and Gaulme, P. and Hekker, S. and Huber, D. and Karoff, C. and Mathur, S. and Mosser, B. and Appourchaux, T. and Ballot, J. and Bedding, T. R. and Bonanno, A. and Broomhall, A.-M. and García, R. A. and Handberg, R. and New, R. and Stello, D. and Régulo, C. and Roxburgh, I. W. and Salabert, D. and White, T. R. and Caldwell, D. A. and Christiansen, J. L. and Fanelli, M. N.},
	month = aug,
	year = {2011},
	note = {arXiv: 1105.0571},
	keywords = {Astrophysics - Solar and Stellar Astrophysics},
	pages = {3539--3551},
	annote = {Comment: 14 pages, 9 figures, accepted for publication in Monthly Notices of the Royal Astronomical Society},
	file = {arXiv\:1105.0571 PDF:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/WQD9M5AS/Verner et al. - 2011 - Global asteroseismic properties of solar-like osci.pdf:application/pdf;arXiv.org Snapshot:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/P2U8FWR2/1105.html:text/html}
