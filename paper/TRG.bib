	journal = {Astronomy and Astrophysics},
	author = {Appourchaux, T. and Chaplin, W. J. and García, R. A. and Gruberbauer, M. and Verner, G. A. and Antia, H. M. and Benomar, O. and Campante, T. L. and Davies, G. R. and Deheuvels, S. and Handberg, R. and Hekker, S. and Howe, R. and Régulo, C. and Salabert, D. and Bedding, T. R. and White, T. R. and Ballot, J. and Mathur, S. and Silva Aguirre, V. and Elsworth, Y. P. and Basu, S. and Gilliland, R. L. and Christensen-Dalsgaard, J. and Kjeldsen, H. and Uddin, K. and Stumpe, M. C. and Barclay, T.},
