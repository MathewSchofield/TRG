	keywords = {methods: statistical, planetary systems, planets and satellites: detection, planets and satellites: dynamical evolution and stability},
	pages = {146},
	file = {NASA/ADS Full Text PDF:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/PZN6S44I/Fabrycky et al. - 2014 - Architecture of Kepler's Multi-transiting Systems..pdf:application/pdf}
