	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Instrumentation and Methods for Astrophysics, Astrophysics - Solar and Stellar Astrophysics},
	annote = {Comment: Six pages, 4 Figures, 2 Tables. Accepted for publication in the The Astrophysical Journal (ApJ). TGAS distance catalogue is available at http://www.mpia.de/homes/calj/tgas\_distances/main.html},
	file = {arXiv\:1609.07369 PDF:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/4QWXU863/Astraatmadja and Bailer-Jones - 2016 - Estimating distances from parallaxes. III. Distanc.pdf:application/pdf;arXiv.org Snapshot:/home/mxs191/.zotero/zotero/9uutfqs0.default/zotero/storage/C3GAFDBW/1609.html:text/html}
